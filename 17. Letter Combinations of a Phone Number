class Solution(object):
    def letterCombinations(self, digits):
        """
        :type digits: str
        :rtype: List[str]
        """
        digit_map = {2:['a','b','c'],3:['d','e','f'],4:['g','h','i'], 5:['j','k','l'],6:['m','n','o'],7:['p','q','r','s'],8:['t','u','v'],9:['w','x','y','z']}
        final,tempo = [], []
        for dig in digits:
            digit = int(dig) 
            if final != []:
                for word in final:
                    for c in digit_map[digit]:
                        tempo.append(word + c)
                final  = tempo
                tempo = []
            else:
                for c in digit_map[digit]:
                    final.append(c)
        return final
